---
import {
  SemiColon,
  OpenFlowerBracket, CloseFlowerBracket,
  DoubleQuote, OpenParenthesis, CloseParenthesis, Equal,
  Colon, Line
} from "../components"
---
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <meta name="description" content="Surendar Yama personal website" />
    <title>Surendar Yama Website | Landing Page</title>
    <link
      rel="icon"
      type="image/svg+xml"
      href="https://surendaryama.github.io/favicon.svg"
    />
    <link
      rel="alternate icon"
      type="image/x-icon"
      href="https://surendaryama.github.io/favicon.ico"
    />
    <link rel="stylesheet" href={Astro.resolve("../styles/global.css")} />
  </head>
  <body class="bg-black text-white font-monaspace-radon p-10 space-y-10">
    <main class="flex flex-col space-y-2 p-2 md:p-10">
        <section class="technologies">
          <h2 class="sm:text-center text-yellow-500 hover:underline">Author Info Config Object <span class="text-red-500">‚ù§</span></h2>
          <div class="flex flex-col p-10 space-y-4">
            <div class="flex text-pink-500 space-x-4">
                <span class="text-white">const</span>
                <span>authorInfoConfig</span>
                <Equal/>
                <OpenFlowerBracket/>
            </div>
            <!-- End of line 1-->
            <div class="flex space-x-6">
                <Line propertyName="name" propertyValue="Surendar Yama" />
            </div>
            <!-- End of line 2 -->
            <div class="flex space-x-6">
                <Line propertyName="desscription" propertyValue="JavaScript Developer" />
            </div>
            <!-- End of line 3 -->
            <div class="flex space-x-6">
              <Line
                  propertyName="youtube"
                  propertyValue="https://www.youtube.com/@surendaryama"
                  link={true}
                  linkUrl="https://www.youtube.com/@surendaryama"
              />
            </div>
            <!-- End of line 4 -->
            <div class="flex space-x-6">
              <Line
                  propertyName="github"
                  propertyValue="https://github.com/surendaryama"
                  link={true}
                  linkUrl="https://github.com/surendaryama"
              />
            </div>
            <!-- End of line 5 -->
            <div class="flex space-x-6">
              <Line
                  propertyName="email"
                  propertyValue="surendaryama@gmail.com"
              />
            </div>
            <!-- End of line 6 -->
            <div class="flex space-x-6">
              <Line
                  propertyName="phoneNumber"
                  propertyValue="+91 8825549745"
              />
            </div>
            <!-- End of line 7 -->
            <div class="flex">
              <CloseFlowerBracket/>
              <SemiColon/>
            </div>
            <!-- End of line 8 -->
            <div class="flex">
              <span class="flex mt-4">
                console.<span class="text-violet-500">log</span>
                <OpenParenthesis/>
                <span class="text-pink-500">authorInfoConfig</span>
                <CloseParenthesis/>
                <SemiColon/>
              </span>
            </div>
            <!-- End of line 9 -->
          </div>
        </section>
        <!--End of Technologies-->
    </main>
    <!-- End of Main  -->
    <footer>

    </footer>
    <!-- End of Footer -->
  </body>
</html>
